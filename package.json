{
	"name": "fast-ecommerce-admin",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"prepare": "npm run smui-theme",
		"start": "node build/index.js"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@roxi/routify": "next",
		"@skeletonlabs/skeleton": "^2.10.0",
		"@skeletonlabs/tw-plugin": "^0.4.0",
		"@smui/data-table": "^7.0.0-beta.16",
		"@smui/icon-button": "^7.0.0-beta.16",
		"@smui/select": "^7.0.0-beta.16",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-node": "^1.0.0-next.106",
		"@sveltejs/kit": "^1.27.4",
		"@tailwindcss/typography": "^0.5.10",
		"@types/node": "^20.12.11",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vincjo/datatables": "^1.14.5",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.30.0",
		"felte": "^1.2.14",
		"flowbite-svelte-icons": "^1.6.1",
		"postcss": "^8.4.33",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"prettier-plugin-tailwindcss": "^0.5.1",
		"smui-theme": "^7.0.0-beta.16",
		"svelte-check": "^3.6.0",
		"sveltekit-superforms": "^2.6.2",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"typescript-svelte-plugin": "^0.3.36",
		"vite": "^4.4.2",
		"vitest": "^0.32.2"
	},
	"type": "module",
	"dependencies": {
		"@canutin/svelte-currency-input": "^0.11.3",
		"@felte/reporter-svelte": "^1.1.11",
		"@felte/validator-zod": "^1.0.17",
		"quill": "2.0.0-rc.3",
		"svelte": "^4.2.16",
		"tailwind-variants": "^0.1.20",
		"zod": "^3.22.4"
	}
}
